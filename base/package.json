  "description": "A simple app built using React, Redux, Express and Node to show data from an SQLite database file.",
  "main": "index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "node index.js",
